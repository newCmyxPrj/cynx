.top-marign{
    margin-top: 20rpx;
}

.address-cell {
  margin-top: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding: 30rpx 20rpx;
  background-color: #fff;
}

.address-content {
  width: 100%;
}

.address-name {
    font-size: 32rpx;
    color: #333333;
    letter-spacing: 0.23px;
    margin-left: 38rpx;
    display: flex;
    justify-content: space-between;
}

.address-str {
    font-size: 28rpx;
    color: #777;
    letter-spacing: 0.2px;
    margin-top: 22rpx;
    margin-left: 38rpx;
}
.address_edit_container
{
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  align-items: center;
  flex-direction: row;
  margin-left:35rpx; 
  justify-content: space-between;
}

.default_icon
{
  width: 35rpx;
  height: 35rpx;
}
.address-edit-container {
    padding:0 6rpx;
    display: flex;
    color: #333;
    font-size: 28rpx;
}

.address-edit-container image {
    width: 26rpx;
    height: 26rpx;
}

.default {
   margin-left: 20rpx;
    border-radius: 10rpx;
    color: #f42c90;
    width: 180rpx;
    height: 100%;
    text-align: center;
    vertical-align: middle;
    font-size: 28rpx;
    letter-spacing: 0.23px; 
    display: flex;
    align-items: center;
}